module.exports = "precision highp float;\nuniform sampler2D tDiffuse;\nuniform float time;\nvarying vec2 vUv;\nvarying float vLighting;\nvarying vec3 vNormal;\nuniform bool isDark;\nfloat random(vec2 co){\n    return fract(sin(dot(co.xy ,vec2(12.9898,78.233))) * 43758.5453);\n}\nvoid main() {\n    vec4 pixel;\n    if(isDark){\n        pixel = vec4(0.,0.,0.,1.);\n    }else{\n        float color = random(vUv+time+(time*vUv.x*sin(vUv.y)));\n        color -= clamp( vLighting, 0., 1.);\n        pixel = vec4(vec3(color * 0.4),1.);\n    }\n    gl_FragColor = pixel ;\n}"