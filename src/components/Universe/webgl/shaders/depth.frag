module.exports = "precision highp float;\nvarying float vDepth;\nvoid main() {\n    vec4 pixel = vec4(1.);\n    float color =  (0. - vDepth ) / 300.;\n    float r = floor(color*100.);\n    float g = floor(color*10000. - r * 100.);\n    float b = floor(color*1000000. - (r * 10000. + g * 100.) );\n    pixel.r = 1./r;\n    pixel.g = 1./g;\n    pixel.b = 1./b;\n    gl_FragColor = pixel;\n}"