module.exports = "precision highp float;\nuniform sampler2D intex;\nuniform vec2 res;\nuniform float split;\nvarying vec2 vUv;\nvoid main() {\n    vec3 mandy = vec3(0.92,0.29,0.35);\n    vec3 aquamarine = vec3(0.49,0.98,0.83);\n    vec2 frag = vec2(1.) / res  * 5. * split;\n    vec3 ch1 =  texture2D( intex , vUv + frag).rgb * mandy;\n    vec3 ch2 =  texture2D( intex , vUv - frag ).rgb * aquamarine;\n    vec3 n =  texture2D( intex , vUv ).rgb;\n    vec3 color = max(ch1, ch2);\n    color = max(color, n);\n    gl_FragColor = vec4(color,1.);\n}"